  ],
  "author": "Olivier Wietrich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bredele/monthend/issues"
  },
  "homepage": "https://github.com/bredele/monthend#readme",
  "devDependencies": {
    "cmplr": "^1.3.0",
    "@types/node": "^24.2.0"
